1.1A.Theatre Square
#include<stdio.h>
int main()
{
	long long int a,n,m;
	scanf("%lld%lld%lld",&n,&m,&a);
	if((m%a)==0&&(n%a)==0)
	{
		
		printf("%lld",(m/a)*(n/a));
	}
	else if((m%a)==0&&(n%a)!=0)
		printf("%lld",(m/a)*(n/a+1));
		else if((n%a)==0&&(m%a)!=0)
			printf("%lld",(m/a+1)*(n/a));
			else
			printf("%lld",(m/a+1)*(n/a+1));
} 
